{
  "name": "@prettier/plugin-ruby",
  "version": "0.7.0",
  "description": "prettier plugin for the Ruby programming language",
  "main": "src/ruby.js",
  "scripts": {
    "test": "jest",
    "build:gem": "pkg --output pkg/prettier --targets=linux,macos,win node_modules/prettier/bin-prettier.js",
    "print": "prettier --plugin=.",
    "rubocop": "run() { prettier --plugin=. $@ | bundle exec rubocop --stdin $1; }; run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prettier/plugin-ruby.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prettier/plugin-ruby/issues"
  },
  "homepage": "https://github.com/prettier/plugin-ruby",
  "dependencies": {
    "prettier": "^1.16.4"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.1.2",
    "jest": "^24.0.0",
    "pkg": "^4.3.7"
  },
  "engines": {
    "node": ">=8.3.0"
  }
}
